<?xml version="1.0" encoding="utf-8"?><extension type="module" version="1.0" method="upgrade">	<!-- Component name -->     <name>FL Video</name>         <!-- Optional  -->     <creationDate>2016-05-17</creationDate>     <author>Fine Line Websites</author>     <authorEmail>info@finelineit.com</authorEmail>     <authorUrl>http://www.finelinewebsites.com</authorUrl>     <copyright>Copyright Info</copyright>     <license>License Info</license>           <version>1.0</version>     <!-- Optional  -->    	<files>		<filename module="mod_fl_video">mod_fl_video.php</filename>		<filename>mod_fl_video.xml</filename>		<filename>index.html</filename>		<folder>tmpl</folder>		<filename>tmpl/default.php</filename>		<filename>tmpl/index.html</filename>		<folder>assets</folder>		<filename>assets/jquery.mb.YTPlayer.min.js</filename>		<folder>assets/css</folder>		<filename>assets/css/jquery.mb.YTPlayer.min.css</filename>		<folder>assets/css/font</folder>		<filename>assets/css/font/ytp-regular.eot</filename>		<filename>assets/css/font/ytp-regular.ttf</filename>		<filename>assets/css/font/ytp-regular.woff</filename>		<folder>assets/css/images</folder>		<filename>assets/css/images/raster_dot.png</filename>		<filename>assets/css/images/raster_dot@2x.png</filename>		<filename>assets/css/images/raster.png</filename>		<filename>assets/css/images/raster@2x.png</filename>			</files><config><fields name="params">	<fieldset name="basic">		<field name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="A suffix to be applied to the css class of the module (table.moduletable), this allows individual module styling" />		<field name="url" type="text" default="" label="YouTube URL" description="https://youtu.be/ABCDE12345" />		<field name="containment" type="text" default="" label="Container" description="What element should the video be contained in. ( body OR .class OR #id )" />		<field name="autoPlay" type="list" default="true" label="Auto Play?">			<option value="true">Yes</option>			<option value="false">No</option>		</field>		<field name="loop" type="list" default="true" label="Loop">			<option value="true">Yes</option>			<option value="false">No</option>		</field>		<field name="mute" type="list" default="true" label="Mute">			<option value="true">Yes</option>			<option value="false">No</option>		</field>		<field name="showControls" type="list" default="false" label="Show Controls">			<option value="true">Yes</option>			<option value="false">No</option>		</field>		<field name="mute" type="list" default="false" label="Show YouTube Logo">			<option value="true">Yes</option>			<option value="false">No</option>		</field>		<field name="stopMovieOnBlur" type="list" default="true" label="Pause On Blur" description="Pause when the window loses focus.">			<option value="true">Yes</option>			<option value="false">No</option>		</field>	</fieldset></fields></config></extension>